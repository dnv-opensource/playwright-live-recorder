{
  "name": "@dnvgl/playwright-live-recorder",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "Adds live coding in testing context from browser console when running tests. Provides programmatically configurable recorder.",
  "main": "dist/main.js",
  "files": [
    "dist/**/*.*"
  ],
  "repository": "https://github.com/dnv-opensource/playwright-live-recorder",
  "author": "DNV",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tslint -p . && tsc && cpx \"src/**/*.{js,css,d.ts}\" dist && cpx \"example/**/*.*\" dist/example",
    "build-fast": "tsc && cpx \"src/**/*.{js,css,d.ts}\" dist && cpx \"example/**/*.*\" dist/example",
    "test": "vitest"
  },
  "devDependencies": {
    "@playwright/test": "^1.0.0",
    "@types/async-lock": "^1.3.0",
    "@types/lodash": "^4.14.185",
    "cpx": "^1.5.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "vitest": "^0.30.1"
  },
  "dependencies": {
    "async-lock": "^1.3.2",
    "chokidar": "^3.5.3",
    "error-stack-parser": "^2.1.4",
    "lodash": "^4.17.21",
    "ts-morph": "^16.0.0",
    "typescript": "^4.8.2"
  }
}
